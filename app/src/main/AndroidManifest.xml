<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kelseyhaydenc196">

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".Activity.AddEditAssessmentActivity"
            android:label="@string/title_activity_add_assessment"
            android:parentActivityName=".Activity.AssessmentDetailActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity
            android:name=".Activity.AssessmentDetailActivity"
            android:label="@string/title_activity_assessment_detail"
            android:parentActivityName=".Activity.AssessmentActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity
            android:name=".Activity.AssessmentActivity"
            android:label="@string/title_activity_assessment_activity"
            android:parentActivityName=".Activity.MainActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity
            android:name=".Activity.AddEditTermActivity"
            android:label="@string/title_activity_add_term"
            android:parentActivityName=".Activity.TermDetailActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity
            android:name=".Activity.TermDetailActivity"
            android:label="@string/title_activity_term_detail"
            android:parentActivityName=".Activity.TermActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity
            android:name=".Activity.TermActivity"
            android:label="@string/title_activity_term_activity"
            android:parentActivityName=".Activity.MainActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity
            android:name=".Activity.AddEditMentorActivity"
            android:label="@string/title_activity_add_mentor"
            android:parentActivityName=".Activity.MentorDetailActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity
            android:name=".Activity.MentorDetailActivity"
            android:label="@string/title_activity_mentor_detail"
            android:parentActivityName=".Activity.MentorActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity
            android:name=".Activity.MentorActivity"
            android:label="@string/title_activity_mentor_activity"
            android:parentActivityName=".Activity.MainActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity
            android:name=".Activity.CourseActivity"
            android:label="@string/title_activity_course_activity"
            android:parentActivityName=".Activity.MainActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity
            android:name=".Activity.CourseDetailActivity"
            android:label="@string/title_activity_course_detail"
            android:parentActivityName=".Activity.CourseActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity
            android:name=".Activity.AddCourseActivity"
            android:label="@string/title_activity_add_course"
            android:parentActivityName=".Activity.CourseActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity
            android:name=".Activity.EditCourseActivity"
            android:label="@string/title_activity_edit_course"
            android:parentActivityName=".Activity.CourseDetailActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />

        <receiver
            android:name=".Utilities.AlertReceiver"
            android:enabled="true"
            android:exported="true">
        </receiver>

        <activity
            android:name=".Activity.MainActivity"
            android:label="@string/title_activity_main_activity"
            android:launchMode="singleTop"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>